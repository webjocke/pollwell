{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pollwell-worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-04",
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "routes": [
    {
      "pattern": "pollwell.se",
      "custom_domain": true
    }
  ],
  "migrations": [
    {
      "new_classes": ["MyDurableObject"],
      "tag": "v1"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "MyDurableObject",
        "name": "MY_DURABLE_OBJECT"
      }
    ]
  },
  "observability": {
    "enabled": true
  }
}
